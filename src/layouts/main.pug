- const linksList = ["profile", "patients", "search"];

doctype html
html
  head
    title Geras
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    script(src="https://use.fontawesome.com/c8751efcd6.js")
    link(rel="stylesheet", href="assets/css/app.css")
    
  body
    //- include common mixins files
    include ./../mixins/drop-list
    include ./../mixins/selector
    include ./../mixins/custom-checkbox
    include ./../mixins/svg-icon
    include ./../mixins/burger-btn
    
    .wrapper
      block sidebar
        include ../partitials/sidebar/sidebar
      main.main-content-wrapper  
        .page-wrapper
          block content
            h3 links to pages
            ul
              each link in linksList
                li
                  a(href=`/${link}.html`) #{link}

    script(src="assets/js/app.js")