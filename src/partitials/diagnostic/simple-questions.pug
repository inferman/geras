-
  const data = {
    "title": "Delayed Word Recall",
    "counter": "5",
    "answersData": [
      {"key": "Expect", "val": "They are vegetables"},
      {"key": "Anwer", "val": "They are fruits"},
      {"key": "Something else", "val": "Very boring descriprion about nothing. Just to get long string, that it. Anyway it's just a characters that hasn't any sens. Daewoo sens. I've heard you have servers"},
    ]
  };

-for (let i=0; i<=3; i++)
  section.spoiler_box
    .spoiler_header.diagnostic_question_header
      .flex
        .flex-item--stretch
          h3 #{data.title}
        .flex-item--shrink
          span #{data.counter}

    .spoiler_body
      .diagnostic_question_body.flex
        .inner-wrapper.inner-wrapper--answers.flex-item--stretch
          article.diagnostic_question_details.flex.flex--wrap
            each answer in  data.answersData
              dl.answer-item
                dt #{answer.key}
                dd #{answer.val}
        .inner-wrapper.inner-wrapper--actions
          button(type="button").g_btn.g_btn--primary Edit
      
